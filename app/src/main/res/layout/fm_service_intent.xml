<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent">
    <ScrollView
        android:layout_width="match_parent"
        android:layout_height="match_parent">
        <RelativeLayout
            android:layout_width="match_parent"
            android:layout_height="match_parent">

            <Button
                android:id="@+id/fm_service_intent_bt_start"
                android:layout_marginTop="10dp"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_alignParentLeft="true"
                android:layout_alignParentStart="true"
                android:layout_alignParentTop="true"
                android:layout_marginLeft="12dp"
                android:layout_marginStart="12dp"
                android:text="启动Service" />
            <TextView
                android:id="@+id/fm_service_intent_tv_ser"
                android:layout_marginTop="10dp"
                android:layout_marginLeft="15dp"
                android:layout_below="@+id/fm_service_intent_bt_start"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:text="TextView" />
            <TextView
                android:id="@+id/fm_service_intent_tv_thread"
                android:layout_marginTop="10dp"
                android:layout_marginLeft="15dp"
                android:layout_below="@+id/fm_service_intent_tv_ser"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:text="TextView" />
            <SeekBar
                android:id="@+id/fm_service_intent_seekbar"
                android:layout_marginTop="10dp"
                android:layout_below="@+id/fm_service_intent_tv_thread"
                android:layout_width="match_parent"
                android:layout_height="wrap_content" />
            <TextView
                android:id="@+id/fm_service_intent_tv_gaishu"
                android:layout_marginTop="10dp"
                android:text="概述"
                android:layout_marginLeft="15dp"
                android:textSize="18sp"
                android:layout_below="@+id/fm_service_intent_seekbar"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content" />
            <TextView
                android:layout_below="@+id/fm_service_intent_tv_gaishu"
                android:layout_margin="15dp"
                android:layout_width="match_parent"
                android:layout_height="wrap_content" />


        </RelativeLayout>




    </ScrollView>


</RelativeLayout>